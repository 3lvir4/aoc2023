use itertools::{Itertools, FoldWhile};



pub fn parse_line(l: String) -> (u32, u8) {
    let (mut hand, mut bid) = l.as_str().split(' ').collect_tuple(2).unwrap();


}

pub fn parse_hand(str: &str) -> u32 {
    let mut chars = str.chars();

    chars
        .counts()
        .into_values()
        .fold_while((0u8, 0u8), |(pairs, triple), n| {
            match n {
                5 => Done(7)
            }
        })
}
